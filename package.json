{
  "name": "knowledge-store",
  "version": "0.1.0",
  "description": "A personal knowledge management system",
  "main": "frontend/src/main.js",
  "scripts": {
    "start": "electron frontend/src/main.js",
    "dev": "cross-env NODE_ENV=development electron frontend/src/main.js",
    "backend": "cd backend && node server.js",
    "backend:dev": "cd backend && npm run dev",
    "frontend": "cd frontend && npm run start",
    "dev:all": "concurrently \"npm run backend\" \"npm run dev\"",
    "start:all": "concurrently \"npm run backend\" \"npm run start\"",
    "install-all": "npm install && cd backend && npm install && cd ../frontend && npm install",
    "test": "cd backend && npm test",
    "postinstall": "npm install concurrently --save-dev"
  },
  "keywords": [
    "knowledge",
    "store",
    "vector",
    "database",
    "electron"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "all": "^0.0.0",
    "cross-env": "^7.0.3",
    "electron": "^27.0.0",
    "express": "^5.1.0",
    "google-auth-library": "^9.15.1"
  },
  "devDependencies": {
    "@babel/core": "^7.27.1",
    "@babel/preset-env": "^7.27.2",
    "babel-jest": "^29.7.0",
    "concurrently": "^8.2.2"
  }
}

{
  "name": "cognivore",
  "version": "0.1.0",
  "description": "A personal knowledge management system",
  "main": "frontend/src/main.js",
  "scripts": {
    "start": "NODE_OPTIONS=\"--inspect --max-old-space-size=4096\" ELECTRON_APP_NAME=Cognivore ELECTRON_BUNDLE_INFO_PLIST=frontend/mac-info.plist electron frontend/src/main.js",
    "dev": "cross-env NODE_ENV=development NODE_OPTIONS=\"--inspect --max-old-space-size=4096\" ELECTRON_APP_NAME=Cognivore ELECTRON_BUNDLE_INFO_PLIST=frontend/mac-info.plist electron frontend/src/main.js",
    "mac-dev": "cd frontend && npm run fix-dock-name && npm run fix-dock-icon && cd .. && npm run dev",
    "mac-start": "cd frontend && npm run fix-dock-name && npm run fix-dock-icon && cd .. && npm run start",
    "backend": "cd backend && node server.js",
    "backend:dev": "cd backend && npm run dev",
    "frontend": "cd frontend && npm run start",
    "dev:all": "concurrently \"npm run backend\" \"npm run dev\"",
    "start:all": "concurrently \"npm run backend\" \"npm run start\"",
    "mac-dev:all": "concurrently \"npm run backend\" \"npm run mac-dev\"",
    "mac-start:all": "concurrently \"npm run backend\" \"npm run mac-start\"",
    "install-all": "npm install && cd backend && npm install && cd ../frontend && npm install",
    "test": "cd backend && npm test",
    "postinstall": "npm install concurrently --save-dev",
    "fix-mac-name": "cd frontend && node scripts/fix-mac-name.js",
    "build": "cd frontend && npm run build"
  },
  "keywords": [
    "knowledge",
    "store",
    "vector",
    "database",
    "electron"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "all": "^0.0.0",
    "cross-env": "^7.0.3",
    "express": "^5.1.0",
    "google-auth-library": "^9.15.1",
    "metascraper-author": "^5.46.15",
    "metascraper-date": "^5.46.15",
    "metascraper-description": "^5.46.15",
    "metascraper-image": "^5.46.15",
    "metascraper-logo": "^5.46.15",
    "metascraper-publisher": "^5.46.15",
    "metascraper-title": "^5.46.15",
    "metascraper-url": "^5.46.15"
  },
  "devDependencies": {
    "@babel/core": "^7.27.1",
    "@babel/preset-env": "^7.27.2",
    "babel-jest": "^29.7.0",
    "buffer": "^6.0.3",
    "concurrently": "^8.2.2",
    "d3": "^7.9.0",
    "electron": "^27.0.0",
    "process": "^0.11.10",
    "querystring": "^0.2.1",
    "querystring-es3": "^0.2.1",
    "url": "^0.11.4"
  }
}
